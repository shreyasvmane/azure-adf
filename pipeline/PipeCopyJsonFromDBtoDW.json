{
	"name": "PipeCopyJsonFromDBtoDW",
	"properties": {
		"activities": [
			{
				"name": "ForEachCheck",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "@pipeline().parameters.ListOfTables",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "CopyDataFromDBtoDW",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "7.00:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "AzureSqlSource",
									"sqlReaderQuery": {
										"value": "SELECT * FROM [@{item().TABLE_SCHEMA}].[@{item().TABLE_NAME}]",
										"type": "Expression"
									},
									"queryTimeout": "02:00:00",
									"partitionOption": "None"
								},
								"sink": {
									"type": "SqlDWSink",
									"allowPolyBase": true,
									"polyBaseSettings": {
										"rejectValue": 0,
										"rejectType": "value",
										"useTypeDefault": true
									}
								},
								"enableStaging": true,
								"stagingSettings": {
									"linkedServiceName": {
										"referenceName": "LinkDataStoreProject",
										"type": "LinkedServiceReference"
									},
									"path": "stagedoutput"
								}
							},
							"inputs": [
								{
									"referenceName": "ProjectDatasetInAzureSql",
									"type": "DatasetReference"
								}
							],
							"outputs": [
								{
									"referenceName": "ProjectDatasetOutSqlPool",
									"type": "DatasetReference",
									"parameters": {
										"TABLE_SCHEMA": {
											"value": "@item().TABLE_SCHEMA",
											"type": "Expression"
										},
										"TABLE_NAME": {
											"value": "@item().TABLE_NAME",
											"type": "Expression"
										}
									}
								}
							]
						}
					]
				}
			}
		],
		"parameters": {
			"ListOfTables": {
				"type": "array",
				"defaultValue": [
					{
						"TABLE_SCHEMA": "BulkCopy",
						"TABLE_NAME": "ProductLines"
					},
					{
						"TABLE_SCHEMA": "BulkCopy",
						"TABLE_NAME": "Customers"
					},
					{
						"TABLE_SCHEMA": "BulkCopy",
						"TABLE_NAME": "Employees"
					},
					{
						"TABLE_SCHEMA": "BulkCopy",
						"TABLE_NAME": "Offices"
					}
				]
			}
		},
		"folder": {
			"name": "ProjectGladiator"
		},
		"annotations": [],
		"lastPublishTime": "2022-07-29T16:23:49Z"
	},
	"type": "Microsoft.DataFactory/factories/pipelines"
}