{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "shreyasvm-datafactory"
		},
		"LinkDataStoreProject_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LinkDataStoreProject'"
		},
		"LinkShreyasDataLake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LinkShreyasDataLake'"
		},
		"LinkShreyasDataStore_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LinkShreyasDataStore'"
		},
		"LinkShreyasSqlDB_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LinkShreyasSqlDB'"
		},
		"LinkTabletoPool_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LinkTabletoPool'"
		},
		"linkStagingBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkStagingBlob'"
		},
		"LinkShreyasDataLake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://shreyasmadl.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "shreyasvm-datafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LinkDataStoreProject_connectionString": {
						"value": "[parameters('LinkDataStoreProject_connectionString')]"
					},
					"LinkShreyasDataLake_accountKey": {
						"value": "[parameters('LinkShreyasDataLake_accountKey')]"
					},
					"LinkShreyasDataStore_connectionString": {
						"value": "[parameters('LinkShreyasDataStore_connectionString')]"
					},
					"LinkShreyasSqlDB_connectionString": {
						"value": "[parameters('LinkShreyasSqlDB_connectionString')]"
					},
					"LinkTabletoPool_connectionString": {
						"value": "[parameters('LinkTabletoPool_connectionString')]"
					},
					"linkStagingBlob_connectionString": {
						"value": "[parameters('linkStagingBlob_connectionString')]"
					},
					"LinkShreyasDataLake_properties_typeProperties_url": {
						"value": "[parameters('LinkShreyasDataLake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "shreyasvm-datafactory_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'shreyasvm-datafactory_ArmTemplate_0')]"
			]
		},
		{
			"name": "shreyasvm-datafactory_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'shreyasvm-datafactory_ArmTemplate_1')]"
			]
		}
	]
}